{
  "name": "foobar-style-chooser",
  "version": "3.0.1",
  "description": "Style chooser for foobar2000 tracks",
  "keywords": [
    "foobar",
    "style",
    "chooser"
  ],
  "author": "Anatoliy Oblaukhov",
  "homepage": "https://github.com/anmiles/foobar-style-chooser",
  "repository": "github:anmiles/foobar-style-chooser",
  "license": "MIT",
  "engines": {
    "node": ">=18.14.2"
  },
  "scripts": {
    "build": "rimraf dist && webpack",
    "build:ci": "webpack",
    "lint": "eslint --ext .js,.ts .",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest --verbose",
    "test:coverage": "npm test -- --coverage",
    "test:ci": "npm test -- --ci --coverage",
    "test:watch": "npm test -- --watch",
    "test:watch:coverage": "npm test -- --watch --coverage",
    "test:report:coverage": "nyc report --nycrc-path ./coverage.config.js -t ./coverage --report-dir ./coverage",
    "start": "node ./server.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "knockout": "^3.5.1",
    "out-url": "^1.2.2"
  },
  "devDependencies": {
    "@anmiles/eslint-config": "^5.0.2",
    "@anmiles/tsconfig": "^2.0.1",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "ajv": "^8.12.0",
    "copy-webpack-plugin": "^12.0.2",
    "eslint": "^8.56.0",
    "eslint-plugin-align-assignments": "^1.1.2",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.6.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "nyc": "^15.1.0",
    "rimraf": "^5.0.5",
    "stylelint": "^16.2.0",
    "stylelint-config-recommended": "^14.0.0",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "webpack": "^5.90.0",
    "webpack-cli": "^5.1.4"
  }
}
