{
  "name": "foobar-style-chooser",
  "version": "1.2.0",
  "description": "Style chooser for foobar2000 tracks",
  "keywords": [
    "foobar",
    "style",
    "chooser"
  ],
  "author": "Anatoliy Oblaukhov",
  "homepage": "https://gitlab.com/anmiles/foobar-style-chooser",
  "repository": "gitlab:anmiles/foobar-style-chooser",
  "license": "MIT",
  "engines": {
    "node": ">=18.14.2"
  },
  "scripts": {
    "build": "rimraf dist && webpack",
    "lint": "eslint --ext .js,.ts .",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest --verbose",
    "test:coverage": "npm test -- --coverage",
    "test:ci": "npm test -- --ci --coverage",
    "test:watch": "npm test -- --watch",
    "test:watch:coverage": "npm test -- --watch --coverage",
    "test:report:coverage": "nyc report --nycrc-path ./coverage.config.js -t ./coverage --report-dir ./coverage",
    "start": "node ./server.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "knockout": "^3.5.1",
    "open": "^8.4.2"
  },
  "devDependencies": {
    "@anmiles/eslint-config": "^2.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.4",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "ajv": "^8.12.0",
    "copy-webpack-plugin": "^11.0.0",
    "eslint": "^8.49.0",
    "eslint-plugin-align-assignments": "^1.1.2",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jest": "^27.2.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "nyc": "^15.1.0",
    "rimraf": "^5.0.1",
    "stylelint": "^15.10.3",
    "stylelint-config-recommended": "^13.0.0",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.4",
    "typescript": "^5.2.2",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  }
}
